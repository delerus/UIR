html
  head
    title= 'UIR_5 Results'
    style
      include style.css
  body
    h2= header
    table
      tr
        th= 'Records'
        th= 'SQL query'
        th= 'SQL table'
        th= 'Time (ms)'
        for res in results
          tr
            td= res.n
            td= res.sql
            td= res.table
            td= res.time
